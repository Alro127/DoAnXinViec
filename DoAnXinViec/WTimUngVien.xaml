<Window x:Class="DoAnXinViec.WTimUngVien"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:local="clr-namespace:DoAnXinViec"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="WTimUngVien" Height="830" Width="1550">
    <Window.Background>
        <ImageBrush ImageSource="/—Pngtree—starry sky galaxy space background_2269978.png"/>
    </Window.Background>
    <Grid Loaded="WTimUngVien_Load">
        <Grid.Background>
            <ImageBrush />
        </Grid.Background>
        <local:UCTrangTimViec x:Name="ucTrangTimViec" Margin="96,0,96,0"/>
        <Button x:Name="btnQuanLyTaiKhoan" Style="{DynamicResource ButtonStyleBoGoc}" Content="Quản lý tài khoản" Margin="0,30,96,0" FontSize="16" FontFamily="Segoe UI Black" Foreground="#FFF6F0F0" BorderBrush="{x:Null}" Height="50" Width="228" HorizontalAlignment="Right" VerticalAlignment="Top" Click="btnQuanLyTaiKhoan_Click" Background="#FF2A8DD3"/>
    </Grid>
</Window>